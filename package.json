{
  "name": "pariflow",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "doppler run -- next dev",
    "dev:monitor": "cd workers/sweep-monitor && doppler run -- npm run dev",
    "dev:local": "next dev",
    "build": "doppler run -- next build",
    "build:local": "next build",
    "vercel-build": "next build",
    "start": "doppler run -- next start",
    "start:local": "next start",
    "lint": "eslint . --ext .ts,.tsx,.js,.jsx",
    "analyze": "ANALYZE=true doppler run -- next build",
    "postinstall": "prisma generate",
    "db:pull": "doppler run -- prisma db pull",
    "db:push": "doppler run -- prisma db push",
    "db:migrate": "doppler run -- prisma migrate dev",
    "db:studio": "doppler run -- prisma studio",
    "setup": "doppler setup --project pariflow --config dev"
